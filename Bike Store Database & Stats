CREATE TABLE bike_store (id INTEGER PRIMARY KEY, name TEXT, price INTEGER, age INTEGER, color NONE); CREATED A TABLE, ORGANIZING INVENTORY FROM THE BIKE STORE, DISTINGUISHING EACH BIKE INTO 5 CHARACTERISTICS.   
